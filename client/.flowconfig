[ignore]
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/flow-typed/npm/.*
.*/**/malformed_package_json/package.json

[include]

[libs]
flow-typed

[lints]
sketchy-null=warn
sketchy-number=warn
unsafe-getters-setters=warn

[options]
module.name_mapper='^\([^\.].*\)$' -> '<PROJECT_ROOT>/src/\1'

module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.mjs
module.file_ext=.json
module.file_ext=.sass
module.file_ext=.scss

[strict]
